
@{
    ViewBag.Title = "Test";
}

<h2>Test</h2>

@model IEnumerable<PleskProblemDemo.ViewModels.TestViewModel>

@if (Model?.Count() == 0 || Model == null)
{
    <div class="alert alert-danger">
        <p>Nothing to show</p>
    </div>
}
else
{

    <table class="table table-striped">
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@($"This is a text view for {item.Text}")</td>
                </tr>
            }
        </tbody>
    </table>

}